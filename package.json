{
  "name": "tishya-foods-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:puppeteer": "npm run test:e2e:puppeteer:all",
    "test:e2e:puppeteer:all": "node tests/puppeteer/run-all-tests.js",
    "test:e2e:shard:1": "node tests/puppeteer/run-shard.js 1",
    "test:e2e:shard:2": "node tests/puppeteer/run-shard.js 2",
    "test:e2e:shard:3": "node tests/puppeteer/run-shard.js 3",
    "test:e2e:shard:4": "node tests/puppeteer/run-shard.js 4",
    "test:performance": "node tests/puppeteer/performance-testing.js",
    "test:a11y": "node tests/puppeteer/accessibility-testing.js",
    "test:smoke": "node tests/puppeteer/smoke-tests.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "analyze": "ANALYZE=true npm run build",
    "prepare": "husky",
    "commit": "cz",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "fresh": "npm run clean && npm install",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "next dev --turbopack",
    "preview": "npm run build && npm run start",
    "check": "npm run lint && npm run type-check && npm run test",
    "check:fix": "npm run lint:fix && npm run type-check",
    "validate": "npm run check && npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit && npx audit-ci --moderate",
    "security:scan": "npm run security:audit && npm run security:licenses",
    "security:licenses": "npx license-checker --summary --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;0BSD;Apache*;Unlicense;WTFPL;CC0-1.0'",
    "security:snyk": "npx snyk test --severity-threshold=medium",
    "security:fix": "npm audit fix && npx snyk wizard",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "bundle:analyze": "npm run analyze && npx webpack-bundle-analyzer .next/analyze/bundle/client.html",
    "docker:dev": "./scripts/docker-dev.sh",
    "docker:dev:start": "./scripts/docker-dev.sh start",
    "docker:dev:stop": "./scripts/docker-dev.sh stop",
    "docker:dev:restart": "./scripts/docker-dev.sh restart",
    "docker:dev:logs": "./scripts/docker-dev.sh logs",
    "docker:dev:build": "./scripts/docker-dev.sh build",
    "docker:dev:shell": "./scripts/docker-dev.sh shell",
    "docker:dev:cleanup": "./scripts/docker-dev.sh cleanup",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:prod:start": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:stop": "docker-compose -f docker-compose.prod.yml down",
    "setup": "./scripts/dev-setup.sh",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^3.10.0",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@modelcontextprotocol/server-puppeteer": "^2025.5.12",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-accordion": "^1.2.1",
